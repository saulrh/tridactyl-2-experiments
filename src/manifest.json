{
  "manifest_version": 2,
  "name": "Tridactyl-Meiosis",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "content.ts" ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "script-src 'unsafe-eval' 'self'; object-src 'self'",
  "permissions": [
    "<all_urls>"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "tridactyl-meiosis@saulrh.com",
      "strict_min_version": "66.0"
    }
  }
}
